{
  "name": "email-validator-ultimate",
  "version": "2.1.0",
  "description": "Advanced email validation library for Node.js. RFC 5322 format validation, MX records, SMTP inbox check, disposable email detection, typo suggestions, risk analysis, Gravatar detection, DNS blacklist check, batch validation, caching, and CLI tool.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "bin": {
    "email-validator-ultimate": "./dist/cli.js",
    "evu": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "email-validator-ultimate",
    "email-validator",
    "email",
    "email-validation",
    "validate-email",
    "email-checker",
    "mx-record",
    "smtp-validation",
    "disposable-email",
    "email-verification",
    "smtp-check",
    "node-email-validator",
    "check-inbox",
    "disposable-email-detection",
    "batch-validation",
    "email-typo",
    "email-suggestion",
    "email-cache",
    "email-normalization",
    "gravatar",
    "risk-analysis",
    "dns-blacklist",
    "email-cli",
    "rfc5322"
  ],
  "author": {
    "name": "Aminul Islam",
    "email": "aminulislamdev23@gmail.com",
    "url": "https://github.com/ai-mehedi/email-validator-ultimate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-mehedi/email-validator-ultimate.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "typescript": "^5.8.3",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/ai-mehedi/email-validator-ultimate/issues"
  },
  "homepage": "https://github.com/ai-mehedi/email-validator-ultimate#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
